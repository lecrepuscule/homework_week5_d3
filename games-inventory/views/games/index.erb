<h2>Game</h2>

<a class="btn" href="/games/new">Add Game</a>

<div class="games">
  <% @games.each do |game| %>
  <div class="row">
    <img src="<%= game.photo_url %>" alt="<%= game.name %> photo">
    <ul>
      <li>
        <h2>
          <a href="/games/<%= game.id %>">
            <%= game.name %>
          </a>
        </h2>
      </li>
      <li>
        <a href="/games/<%= game.id %>/edit">
          Edit
        </a>
      </li>
      <li>
        <li>
          <form method="/games/<%= game.id %>/delete" action="post">
            <button>Delete</button>
          </form>
        </li>
      </li>
    </ul>
  </div>
  <% end %>
</div>
